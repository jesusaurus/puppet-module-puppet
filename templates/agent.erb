[agent]
  certname      = <%= scope.lookupvar("fqdn") %>
  report        = <%= scope.lookupvar("puppet::params::report") %>
  show_diff     = <%= not scope.lookupvar("puppet::params::quiet") %>
  environment   = <%= scope.lookupvar("puppet::params::default_environment") %>
  pluginsync    = <%= scope.lookupvar("puppet::params::pluginsync") %>

